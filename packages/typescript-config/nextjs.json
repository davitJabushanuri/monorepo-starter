{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "ES2023"
    ],
    "plugins": [{ "name": "next" }],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true,
    "target": "ES2022",
    "incremental": true,
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true
  }
}
