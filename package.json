{
  "name": "monorepo-starter",
  "private": true,
  "scripts": {
    "start": "turbo run start",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "pnpm dlx biome check",
    "lint:fix": "pnpm dlx biome check --write",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:headless": "turbo run test:headless",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage",
    "test:e2e": "turbo run test:e2e",
    "test:e2e:dev": "turbo run test:e2e:dev",
    "test:e2e:debug": "turbo run test:e2e:debug",
    "test:e2e:ci": "turbo run test:e2e:ci",
    "test:e2e:install": "turbo run test:e2e:install",
    "test:e2e:install:ci": "turbo run test:e2e:install:ci",
    "prepare": "husky",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "turbo run db:studio",
    "db:seed": "turbo run db:seed"
  },
  "devDependencies": {
    "turbo": "^2.5.8",
    "@biomejs/biome": "2.2.4",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "husky": "^9.1.7",
    "semantic-release": "^24.2.9"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.18.0",
  "version": "1.2.0"
}
